{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "custom/video-background-block",
    "title": "Video Background Block",
    "category": "media",
    "icon": "format-video",
    "description": "A block with a video background, text, a button, and an arrow.",
    "supports": {
      "color": {
        "text": true,
        "background": false
      },
      "typography": {
        "fontSize": true
      },
      "spacing": {
        "padding": true,
        "margin": true
      }
    },
    "attributes": {
      "videoUrl": {
        "type": "string",
        "default": ""
      },
      "title": {
        "type": "string",
        "default": "Smartare IT med cirkulära lösningar"
      },
      "subtitle": {
        "type": "string",
        "default": "Vi hjälper företag med helhetslösningar för hållbar IT-konsumtion."
      },
      "buttonText": {
        "type": "string",
        "default": "Läs mer"
      },
      "buttonUrl": {
        "type": "string",
        "default": "#"
      }
    },
    "editorScript": "file:./index.js",
    "style": "file:./style.css"
  }
  